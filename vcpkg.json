{
  "name": "nebulastream-dependencies",
  "version-string": "v25",
  "homepage": "https://nebula.stream",
  "description": "Data Management for the Internet of Things.",
  "supports": "x64 & (linux | osx)",
  "builtin-baseline": "d51b969a7db84d56d2083bb22b2f95254bdc4c3f",
  "dependencies": [
    "z3",
    "zeromq",
    "cppzmq",
    "grpc",
    "libevent",
    "fmt",
    "paho-mqttpp3",
    "mbedtls",
    "open62541",
    "libsodium",
    "eigen3",
    "gtest",
    "antlr4",
    "spdlog",
    "folly",
    "jemalloc-nes",
    {
      "name": "cppkafka",
      "platform": "!osx"
    },
    {
      "name": "libiconv",
      "platform": "osx"
    },
    {
      "name": "asio",
      "default-features": false,
      "features": ["openssl"],
      "platform": "osx"
    },
    {
      "name": "llvm",
      "default-features": false,
      "features": ["clang","openmp", "default-options", "lld", "tools", "mlir", "target-x86"],
      "platform": "x64"
    },
    {
      "name": "llvm",
      "default-features": false,
      "features": ["clang","openmp", "default-options", "lld", "tools", "mlir", "target-aarch64"],
      "platform": "arm64"
