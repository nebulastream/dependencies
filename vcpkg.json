{
  "name": "nebulastream-dependencies",
  "version-string": "v7",
  "homepage": "https://nebula.stream",
  "description": "Data Management for the Internet of Things.",
  "supports": "x64 & (linux | osx)",
  "dependencies": [
    "z3",
    "boost-system",
    "boost-process",
    "boost-thread",
    "boost-program-options",
    "boost-filesystem",
    "boost-chrono",
    "zeromq",
    "cppzmq",
    "log4cxx",
    "grpc",
    "cpprestsdk",
    "libevent",
    "fmt",
    "paho-mqtt",
    "paho-mqttpp3",
    "mbedtls",
    "open62541",
    "libsodium",
    "eigen3",
    "folly", 
    {
      "name": "cppkafka",
      "platform": "!osx"
    },
    {
      "name": "libiconv",
      "platform": "osx"
    },
    {
      "name": "asio",
      "default-features": false,
      "features": ["openssl"],
      "platform": "osx"
    },
    {
      "name": "llvm",
      "default-features": false,
      "features": ["clang","libunwind","openmp", "mlir", "target-x86"],
      "platform": "x64"
    },
    {
      "name": "llvm",
      "default-features": false,
      "features": ["clang","libunwind","openmp", "mlir", "target-aarch64"],
      "platform": "arm64"
    }
  ]
}
