{
  "name": "nebulastream-dependencies",
  "version-string": "v15",
  "homepage": "https://nebula.stream",
  "description": "Data Management for the Internet of Things.",
  "supports": "x64 & (linux | osx)",
  "builtin-baseline": "daa7215f3d68c2a2f2e18c398cb73dab63cda829",
  "dependencies": [
    "z3",
    "boost-system",
    "boost-process",
    "boost-thread",
    "boost-program-options",
    "boost-filesystem",
    "boost-chrono",
    "zeromq",
    "cppzmq",
    "log4cxx",
    "grpc",
    "cpprestsdk",
    "libevent",
    "fmt",
    "paho-mqttpp3",
    "mbedtls",
    "open62541",
    "libsodium",
    "eigen3",
    "gtest",
    "antlr4",
    "folly", 
    "jemalloc-nes", 
    {
      "name": "cppkafka",
      "platform": "!osx"
    },
    {
      "name": "libiconv",
      "platform": "osx"
    },
    {
      "name": "asio",
      "default-features": false,
      "features": ["openssl"],
      "platform": "osx"
    },
    {
      "name": "llvm",
      "default-features": false,
      "features": ["clang","openmp", "default-options", "lld", "tools", "mlir", "target-x86"],
      "platform": "x64"
    },
    {
      "name": "llvm",
      "default-features": false,
      "features": ["clang","openmp", "default-options", "lld", "tools", "mlir", "target-aarch64"],
      "platform": "arm64"
    }
  ]
}
